  <connectionStrings>
    <clear />
    <add name="OpenSPDBContext" providerName="System.Data.SqlClient" connectionString="server=.;database=DBManager;uid=sa;pwd=1234" />
  </connectionStrings>
  

  
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="CacheTime" value="7200" />
    
  </appSettings>
  <system.web>
    <caching>
      <sqlCacheDependency enabled="true" pollTime="6000">
        <databases>
          <clear />
          <add name="TestCache" connectionStringName="OpenSPDBContext" />
        </databases>
      </sqlCacheDependency>
    </caching>
    <compilation debug="true" targetFramework="4.5.1" />
    <httpRuntime targetFramework="4.5.1" />
    <httpModules>
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web" />
